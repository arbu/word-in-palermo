{
  "private": true,
  "name": "word-in-palermo",
  "version": "0.1.0",
  "description": "a multiplayer game about guessing words",
  "main": "lib/index.js",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "loadtest": "colyseus-loadtest test/load.ts --room palermo --numClients 2",
    "test": "mocha --require ts-node/register test/*.ts && npm run loadtest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "author": "Aaron Bulmahn",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/arbu/word-in-palermo/issues"
  },
  "homepage": "https://github.com/arbu/word-in-palermo",
  "engines": {
    "node": ">=11.14.0"
  },
  "devDependencies": {
    "@colyseus/loadtest": "^0.14.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "colyseus.js": "^0.14.0",
    "eslint": "^7.11.0",
    "gts": "^3.0.1",
    "mocha": "^8.1.3",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.12.2",
    "@colyseus/schema": "^1.0.0",
    "@colyseus/social": "^0.10.9",
    "colyseus": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "xstate": "^4.13.0"
  }
}
